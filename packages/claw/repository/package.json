{
  "name": "@netko/claw-repository",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "db:generate": "prisma generate --schema ./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --skip-generate --schema ./src/prisma/schema.prisma",
    "db:deploy": "prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "db:seed": "bun run ./src/prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@netko/claw-config": "workspace:*",
    "@prisma/client": "^6.16.2",
    "ioredis": "^5.8.0",
    "prisma": "^6.16.2",
    "zod": "^4.1.11"
  }
}
