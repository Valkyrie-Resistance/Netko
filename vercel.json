{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/ws/:match*",
      "destination": "https://api.netko.pw/ws/:match*"
    },
    {
      "source": "/api/:match*",
      "destination": "https://api.netko.pw/api/:match*"
    },
    {
      "source": "/auth/api/:match*",
      "destination": "https://api.netko.pw/auth/api/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
